
btn_add_click:function($event){
	if(!$model.entity.projectId){
		Messenger.post({type:'info',message:'模块名称不能为空！'});
		return;
	}else if(!$model.entity.year){
		Messenger.post({type:'info',message:'截止日期不能为空！'});
		return;
	
	}
	$model.entity.$save(function(){
		Modal.instance.close();
		Messenger.post({type:'success',message:'新增成功'});
	});
},
btn_edit_click:function($event){
	Modal.instance.close();
}